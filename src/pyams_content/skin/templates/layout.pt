				<!-- end content -->

			</div>
			<!-- end main panel -->

			<!-- Page footer -->
<!--?			<i tal:omit-tag="">${structure:provider:pyams.footer}</i>-->
<!--?			<i tal:omit-tag="">${structure:provider:pyams.alerts}</i>-->
